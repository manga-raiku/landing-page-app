{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "none",
  "printWidth": 20,
  "semi": false,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "pluginSearchDirs": [
    "."
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "prettier"
      }
    }
  ]
}
